{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "devCommand": "npm start",
  "installCommand": "npm install",
  "framework": "create-react-app",
  "env": {
    "REACT_APP_WS_URL": {
      "description": "WebSocket URL for backend",
      "value": "wss://connect4-backend.onrender.com/ws"
    },
    "REACT_APP_API_URL": {
      "description": "API URL for backend",
      "value": "https://connect4-backend.onrender.com"
    }
  }
}
